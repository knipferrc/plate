{
	"compileOnSave": false,
	"compilerOptions": {
		"jsx": "preserve",
    "jsxFactory": "preact.h",
		"jsxFragmentFactory": "preact.Fragment",
    "types": ["preact/jsx-runtime"],
		"allowJs": true,
		"checkJs": true,
		"strict": true,
		"noEmit": true,
		"moduleResolution": "node",
		"target": "ESNext",
		"module": "esnext",
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"downlevelIteration": true
	},
	"typeAcquisition": {
		"enable": true
	}
}
